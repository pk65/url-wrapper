{
    "manifest_version": 2,
    "name": "URL Unwrapper",
    "description": "Unwraps link with parameter 'url'",
    "version": "0.1.0",
    "icons": {
        "48": "unwrapper-logo-48.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "clipboardWrite"
    ]
}
